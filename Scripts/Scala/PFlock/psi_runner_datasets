#!/bin/bash


N=$1


ES=( 10 12 14 16 18 20) # epsilon
MS=( 3 ) # mu

for dataset in $HOME/Research/Datasets/LA/cells/*.tsv
do
    for n in `seq 1 $N`
    do
	for e in "${ES[@]}"
	do
		for m in "${MS[@]}"
		do
			echo "bash/checker_scala --dataset $dataset --epsilon $e --mu $m --tester"
			bash/checker_scala --dataset $dataset --epsilon $e --mu $m --tester
		done
	done
    done
done

dir="$HOME/Research/Datasets/LA/cells/"
while read -r f; do
    echo "$f"
done < <(find "$dir" -printf "%k KB %p\n" | sort -nr)
