  #!/bin/bash

DATASET=$1
N=$2

ES=( 5 10 15 20 25 30 35 40 45 50) # epsilon

for n in $(seq 1 "$N")
do
	for e in "${ES[@]}"
	do
		echo ""
		echo "Running ./pflocks_spark --master local[10] --input $DATASET --epsilon $e (iteration $n)"
		echo ""
		./pflocks_spark --master local[10] --input "$DATASET" --epsilon "$e" -e
	done
done
